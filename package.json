{
  "name": "cluetrainer",
  "scripts": {
    "build-libs": "npm run build -w alt1",
    "build": "npm run build-libs && node -r ts-node/register ./node_modules/webpack/bin/webpack.js --node-env development",
    "build-beta": "npm run build-libs && node -r ts-node/register ./node_modules/webpack/bin/webpack.js --node-env beta",
    "build-production": "npm run build-libs && node -r ts-node/register ./node_modules/webpack/bin/webpack.js",
    "watch": "npm run build -- --watch",
    "runlocal": "electron dist/electron.bundle.js"
  },
  "workspaces": [
    "lib/*"
  ],
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "assert": "^2.1.0",
    "assert-browserify": "^2.0.0",
    "buffer": "^6.0.3",
    "byte-base64": "^1.1.0",
    "dompurify": "3.2.4",
    "fuzzysort": "^3.1.0",
    "jquery": "^3.7.1",
    "leaflet": "^1.9.4",
    "levenshtein-edit-distance": "^3.0.1",
    "lodash": "^4.17.21",
    "pako": "^2.1.0",
    "process": "^0.11.10",
    "timers-browserify": "^2.0.12",
    "tippy.js": "^6.3.7",
    "uuid": "^11.0.3",
    "alt1": "workspace:alt1"
  },
  "devDependencies": {
    "@alt1/imagedata-loader": "^1.0.0-alpha.6",
    "@types/dompurify": "^3.0.5",
    "@types/jquery": "^3.5.29",
    "@types/leaflet": "^1.9.3",
    "@types/lodash": "^4.14.194",
    "@types/node": "^12.12.54",
    "@types/pako": "^2.0.0",
    "@types/uuid": "^9.0.7",
    "@types/webpack": "^5.28.0",
    "circular-dependency-plugin": "^5.2.2",
    "copy-webpack-plugin": "^11.0.0",
    "csstype": "^3.1.3",
    "file-loader": "^5.1.0",
    "json-loader": "^0.5.7",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.6.3",
    "webpack": "5.96.1",
    "webpack-cli": "^5.1.4",
    "wrangler": "^4.47.0"
  }
}
